{
  "name": "untested",
  "version": "0.1.5",
  "description": "Orders tests for a source code change based on code coverage analysis.",
  "author": "Gleb Bahmutov <gleb.bahmutov@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/bahmutov/untested.git"
  },
  "scripts": {
    "test": "grunt && node untested.js --info"
  },
  "pre-commit": "npm test",
  "pre-push": "rm -rf node_modules && npm install && npm test",
  "keywords": [
    "js",
    "javascript",
    "testing",
    "quality",
    "changelist",
    "commit"
  ],
  "dependencies": {
    "optimist": "~0.6.0",
    "lodash": "~2.4.1",
    "gt": "~0.8.21",
    "lasso-node": "~0.1.11",
    "ggit": "0.0.5",
    "check-types": "~1.1.1"
  },
  "engines": {
    "node": ">= 0.8.0"
  },
  "license": "MIT",
  "preferGlobal": "true",
  "bin": {
    "untested": "./untested.js"
  },
  "main": "./untested.js",
  "devDependencies": {
    "grunt-bump": "0.0.13",
    "grunt-contrib-jshint": "~0.7.2",
    "grunt": "~0.4.2",
    "pre-git": "0.0.14"
  }
}
